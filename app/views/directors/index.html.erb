<h2>List of all directors</h2>
<a href="javascript:history.back()">Go Back</a>

<table border="1">
  <tr>
    <th>ID</th>
    <th>Image</th>
    <th>Name</th>
    <th>DOB</th>
    <th></th>
  </tr>

  <% @directors.each do |director| %>
    <tr>
      <td><%= director[:id] %></td>
      <td><img src="<%= director[:image] %>" width="100" /></td>
      <td><%= director[:name] %></td>
      <td><%= director[:dob] %></td>
      <td><a href="/directors/<%= director[:id] %>">Show details</a></td>
    </tr>
  <% end %>
</table>
